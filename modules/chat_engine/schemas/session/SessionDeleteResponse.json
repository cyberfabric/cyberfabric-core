{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "gts://gts.x.chat_engine.session.delete_response.v1",
  "title": "SessionDeleteResponse",
  "description": "Response after deleting a session",
  "type": "object",
  "properties": {
    "session_id": {
      "$ref": "../common/GtsSessionId.json",
      "description": "Session that was deleted"
    },
    "deleted": {
      "type": "boolean",
      "const": true,
      "description": "Confirmation that session was deleted"
    },
    "deletion_type": {
      "$ref": "../common/DeletionType.json",
      "description": "Type of deletion performed"
    },
    "deleted_at": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when session was deleted"
    },
    "scheduled_hard_delete_at": {
      "type": ["string", "null"],
      "format": "date-time",
      "description": "When soft deleted, shows scheduled permanent deletion time"
    }
  },
  "required": ["session_id", "deleted", "deletion_type", "deleted_at"],
  "additionalProperties": false
}
